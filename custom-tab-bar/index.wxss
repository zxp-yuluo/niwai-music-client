/* custom-tab-bar/index.wxss */
.tab-bar {
    position: fixed;
    bottom: 0px;
    left: 0px;
    right: 0px;
    width: 100%;
    height: 120rpx;
    /* background: white; */
    display: flex;
    flex-direction: row;
    /* padding-bottom: env(safe-area-inset-bottom); */
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: cover;
}

.tab-bar-border {
    /* background-color: #e4e4e4; */
    position: absolute;
    left: 0px;
    top: 0px;
    width: 100%;
    height: 18rpx;
}

.tab-bar-item {
    flex: 1;
    text-align: center;
    display: flex;
    align-items: center;
    flex-direction: column;
    padding-top: 16rpx;
}

.cover-image {
    width: 46rpx;
    height: 46rpx;
}

.coverStyle {
    font-size: 22rpx;
}

.centerImage {
    width: 120rpx;
    height: 120rpx;
    position: absolute;
    top: -50rpx;
    /* transform: translateX(1rpx); */
    border-radius: 50%;
}

.imageRotate {
    animation: rotateAnimation 20s infinite linear; 
}
.stop {
    animation-play-state: paused;
}
.start {
    animation-play-state: running;
}

@keyframes rotateAnimation {
    from {transform: rotate(0deg);}
    to {transform: rotate(360deg);}
}